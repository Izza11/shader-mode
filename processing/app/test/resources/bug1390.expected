  @SuppressWarnings("serial") DIV_10(10) { protected int apply(int x) { return x / y; } },
  SUB_8(8)                               { protected int apply(int x) { return x - y; } };
  
  final int y;
  
  Operation(int y) {
  	this.y = y;
  }
  
